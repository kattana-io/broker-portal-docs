    "apiBatchOrder": {
      "type": "object",
      "properties": {
        "text": {
          "type": "string",
          "title": "User defined information. If not empty, must follow the rules below:\n1. prefixed with `t-`\n2. no longer than 28 bytes without `t-` prefix\n3. can only include 0-9, A-Z, a-z, underscore(_), hyphen(-) or dot(.)"
